{
  "name": "patatabot",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/Orhideon/patatabot.git",
  "author": "BuildTools <unconfigured@null.spigotmc.org>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-aqua": "^7.1.2"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off"
    }
  }
}
