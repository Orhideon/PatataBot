{
  "name": "patatabot",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/Orhideon/patatabot.git",
  "author": "BuildTools <unconfigured@null.spigotmc.org>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js",
    "mongoose": "^5.7.13",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-aqua": "^7.1.2",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.3.0",
    "prettier": "1.16.4"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "no-dynamic-require": "off"
    }
  }
}
